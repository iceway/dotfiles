###########################################################################

snippet testcode "a test code set" b
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>

int main(int argc, char *argv[])
{
	${VISUAL}$0

	return 0;
}
endsnippet

snippet struct "define a new struct" b
struct _$1 {
	$2;
};
typedef struct _$1 ${1:STRUCT_NAME};
${VISUAL}$0
endsnippet

snippet Struct "define a new struct with next" b
struct _$1 {
	struct _$1 *next;
	$2;
};
typedef struct _$1 ${1:STRUCT_NAME};
${VISUAL}$0
endsnippet

